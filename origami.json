{
	"description": "Share module",
	"origamiType": "module",
	"origamiCategory": "components",
	"origamiVersion": 1,
	"support": "https://github.com/FTChinese/ftc-share/issues",
	"supportStatus": "active",
	"browserFeatures": {
		"required": [
			"document.queryselector",
			"Element.prototype.closest"
		],
		"optional": []
		},
	"demosDefaults": {
		"sass": "demos/src/demo.scss",
		"js": "demos/src/demo.js"
	},
	"demos": [
		{
			"name": "share",
			"template": "demos/src/share-js.html",
			"description": "default theme",
			"share": {
				"sprite": false,
				"theme": "theme-default"
			}
		},
		{
			"name": "share-dark",
			"template": "demos/src/share-js.html",
			"description": "dark theme",
			"share": {
				"spirte": false,
				"theme": "theme-dark"
			}
		},
		{
			"name": "share-light",
			"template": "demos/src/share-js.html",
			"description": "light theme",
			"share": {
				"sprite": false,
				"theme": "theme-light"
			}
		},
		{
			"name": "sprite",
			"template": "demos/src/share-sprite.html",
			"description": "use svg symbol as icon image",
			"share": {
				"sprite": true,
				"theme": "sprite-default"
			}
		},
		{
			"name": "sprite-dark",
			"template": "demos/src/share-sprite.html",
			"description": "use svg symbol as icon image",
			"share": {
				"sprite": true,
				"theme": "sprite-dark"	
			}
			
		},
		{
			"name": "sprite-light",
			"template": "demos/src/share-sprite.html",
			"description": "use svg symbol as icon image",
			"share": {
				"sprite": true,
				"theme": "sprite-light"	
			}
		}
	]
}
