{
	"description": "Share module",
	"origamiType": "module",
	"origamiCategory": "components",
	"origamiVersion": 1,
	"support": "https://github.com/FTChinese/ftc-share/issues",
	"supportStatus": "active",
	"browserFeatures": {
		"required": [
			"document.queryselector",
			"Element.prototype.closest"
		],
		"optional": []
		},
	"demosDefaults": {
		"sass": "demos/src/demo.scss",
		"js": "demos/src/demo.js"
	},
	"demos": [
		{
			"name": "share",
			"template": "demos/src/share.html",
			"data": "demos/src/data.json",
			"description": "default theme"
		},
		{
			"name": "share-dark",
			"template": "demos/src/share-js.html",
			"theme": "theme-dark",
			"data": "demos/src/data.json",
			"description": "dark theme"
		},
		{
			"name": "share-light",
			"template": "demos/src/share-js.html",
			"theme": "theme-light",
			"data": "demos/src/data.json",
			"description": "light theme"
		},
		{
			"name": "share-tint",
			"template": "demos/src/share-js.html",
			"data": "demos/src/data.json",
			"description": "tint theme",
			"theme": "theme-tint"
		},
		{
			"name": "svg-symbol",
			"template": "demos/src/share.html",
			"data": "demos/src/data.json",
			"description": "use svg symbol as icon image",
			"svgSymbol": true,
			"theme": "symbol-default"
		},
		{
			"name": "svg-symbol-light",
			"template": "demos/src/share.html",
			"data": "demos/src/data.json",
			"description": "use svg symbol as icon image",
			"svgSymbol": true,
			"theme": "symbol-light"
		},
		{
			"name": "svg-symbol-dark",
			"template": "demos/src/share.html",			
			"data": "demos/src/data.json",
			"description": "use svg symbol as icon image",
			"svgSymbol": true,
			"theme": "symbol-dark"
		},
		{
			"name": "svg-symbol-tint",
			"template": "demos/src/share.html",
			"data": "demos/src/data.json",
			"description": "use svg symbol as icon image",
			"svgSymbol": true,
			"theme": "symbol-tint"
		}
	]
}